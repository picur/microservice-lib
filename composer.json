{
  "name": "phppro/microservice-lib",
  "description": "PHPPRO MicroService common library",
  "require": {
    "php": ">=5.5.9",
    "symfony/console": "2.*",
    "symfony/config": "2.*",
    "symfony/dependency-injection": "2.*",
    "monolog/monolog": "1.*",
    "symfony/yaml": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Olivier Hoareau",
      "email": "olivier@phppro.fr"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "bin-dir": "bin",
    "process-timeout": 900
  },
  "autoload": {
    "psr-4": {
      "Phppro\\MicroService\\": "src/"
    }
  },
  "prefer-stable": true
}
